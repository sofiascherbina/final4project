<div>
    <h3>{{name}}</h3>
    <ul>
        <li><p>Id <span>{{id}}</span></p></li>
        <li><p>Air date <span>{{air_date}}</span></p></li>
    </ul>
    <h3>Characters</h3>
    <p>Major Characters</p>
    <ul>
        {{#each characters}}
            <li>
                <img src="{{image}}" alt="{{url}}" width="50px" height="50px">
                <p>{{name}}</p>
            </li>
        {{/each}}
    </ul>
</div>